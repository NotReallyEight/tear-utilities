{
  "name": "tear-utilities",
  "version": "0.0.1",
  "description": "Utility bot for Tear Paradise",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --fix",
    "prepare": "npm test && husky install",
    "prettier": "prettier --write src/**/*.ts",
    "test": "eslint src && tsc --noEmit && npm run prettier",
    "start": "npm run build && node --enable-source-maps ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotReallyEight/tear-utilities.git"
  },
  "keywords": [
    "utility-bot"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "NotReallyEight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NotReallyEight/tear-utilities/issues"
  },
  "homepage": "https://github.com/NotReallyEight/tear-utilities#readme",
  "dependencies": {
    "@discordjs/rest": "^0.2.0-canary.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "^16.11.19",
    "bufferutil": "^4.0.6",
    "chalk": "^4.1.2",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "prettier": "^2.5.1",
    "tsup": "^5.11.11",
    "utf-8-validate": "^5.0.8",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/prettier": "^2.4.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.0",
    "node": "^16.13.1"
  }
}