{
  "name": "tear-utilities",
  "version": "0.0.1",
  "description": "Utility bot for Tear Paradise",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --fix",
    "prepare": "npm test && husky install",
    "prettier": "prettier --write src/**/*.ts",
    "test": "eslint src && tsc --noEmit && npm run prettier",
    "start": "npm run build && node --enable-source-maps ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotReallyEight/tear-utilities.git"
  },
  "keywords": [
    "utility-bot"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "NotReallyEight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NotReallyEight/tear-utilities/issues"
  },
  "homepage": "https://github.com/NotReallyEight/tear-utilities#readme",
  "dependencies": {
    "@discordjs/rest": "^0.5.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^16.11.41",
    "bufferutil": "^4.0.6",
    "canvas": "^2.9.1",
    "chalk": "^4.1.2",
    "discord-api-types": "^0.37.17",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "mongodb": "4.7",
    "prettier": "^2.7.1",
    "tsup": "^6.1.2",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/prettier": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.1",
    "node": "^16.15.1",
    "typescript": "^4.7.4"
  }
}
